{
  "name": "console-io",
  "version": "2.7.14",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Web console",
  "homepage": "http://cloudcmd.io",
  "bin": {
    "console": "bin/console.js",
    "console_": "legacy/bin/console.js"
  },
  "main": "lib/server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudcmd/console.git"
  },
  "scripts": {
    "test": "gulp default",
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel bin -d legacy/bin",
    "wisdom": "npm run build",
    "build": "redrun 6to5:* mkdir:* legacy:*",
    "legacy:index": "echo \"module.exports = require('./lib/server');\" > legacy/index.js",
    "legacy:package": "echo \"module.exports = require('../package');\" > legacy/package.js",
    "mkdir:json": "mkdir -p legacy/json",
    "legacy:modules": "echo \"module.exports = require('../../json/modules');\" > legacy/json/modules.js"
  },
  "keywords": [
    "console",
    "terminal",
    "express"
  ],
  "dependencies": {
    "debug": "^2.3.0",
    "express": "^4.14.0",
    "join-io": "~1.4.0",
    "mollify": "~1.0.3",
    "rendy": "~1.1.0",
    "socket.io": "^1.5.0",
    "spawnify": "~2.3.0",
    "tildify": "~1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "bower": "^1.5.1",
    "gulp": "^3.9.0",
    "gulp-jshint": "^2.0.0",
    "gulp-recess": "^1.2.0",
    "jshint": "^2.8.0"
  },
  "license": "MIT"
}
