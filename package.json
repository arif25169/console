{
  "name": "console-io",
  "version": "3.1.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Web console",
  "homepage": "http://cloudcmd.io",
  "bin": {
    "console": "bin/console.js",
    "console_": "legacy/bin/console.js"
  },
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudcmd/console.git"
  },
  "scripts": {
    "start": "node bin/console",
    "start:dev": "NODE_ENV=development npm start",
    "build:client": "webpack --progress",
    "build:client:dev": "NODE_ENV=development redrun build:client",
    "watcher": "nodemon -w client -w server --exec",
    "watch:lint": "npm run watcher -- 'npm run lint'",
    "watch:client": "redrun build:client -- --watch",
    "watch:client:dev": "redrun build:client:dev -- --watch",
    "lint:eslint-bin": "eslint --rule 'no-console:0' bin",
    "lint:eslint": "eslint --config .client.eslintrc server webpack.config.js",
    "lint": "redrun lint:*",
    "6to5:lib": "babel server -d server_",
    "6to5:bin": "babel bin -d legacy/bin",
    "wisdom": "npm run build",
    "build": "redrun 6to5:* build:* legacy:*",
    "legacy:index": "echo \"module.exports = require('../server_');\" > legacy/index.js"
  },
  "keywords": [
    "console",
    "terminal",
    "express"
  ],
  "dependencies": {
    "currify": "^2.0.0",
    "debug": "^2.3.0",
    "express": "^4.14.0",
    "join-io": "^1.4.0",
    "mollify": "^1.0.3",
    "rendy": "^1.1.0",
    "socket.io": "^2.0.1",
    "spawnify": "^3.0.0",
    "tildify": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "bower": "^1.5.1",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "redrun": "^5.9.3",
    "webpack": "^2.6.1"
  },
  "license": "MIT"
}
